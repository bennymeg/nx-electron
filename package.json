{
  "name": "nx-electron",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "description": "Electron Plugin for Nx",
  "author": "Benny Megidish",
  "scripts": {
    "start": "nx run nx-electron:serve",
    "build": "nx run nx-electron:build",
    "link": "npm link ./dist/packages/nx-electron/",
    "test": "nx run nx-electron:test",
    "lint": "nx run nx-electron:lint",
    "publish": "cd dist/packages/nx-electron && npm publish",
    "publish:alpha": "cd dist/packages/nx-electron && npm publish --tag alpha",
    "publish:beta": "cd dist/packages/nx-electron && npm publish --tag beta",
    "release": "cd packages/nx-electron && npm run release"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/core": "16.0.2",
    "@angular-devkit/schematics": "16.0.2",
    "@nx/angular": "16.2.2",
    "@nx/devkit": "16.2.2",
    "@nx/eslint-plugin": "16.2.2",
    "@nx/jest": "16.2.2",
    "@nx/js": "16.2.2",
    "@nx/linter": "16.2.2",
    "@nx/node": "16.2.2",
    "@nx/plugin": "16.2.2",
    "@nx/workspace": "16.2.2",
    "@schematics/angular": "16.0.2",
    "@swc-node/register": "^1.6.5",
    "@swc/core": "^1.3.59",
    "@types/jest": "29.5.1",
    "@types/node": "20.2.3",
    "@types/webpack": "^5.28.1",
    "@types/webpack-node-externals": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "5.59.7",
    "@typescript-eslint/parser": "5.59.7",
    "bufferutil": "^4.0.7",
    "canvas": "^2.11.2",
    "electron": "^25.3.2",
    "eslint": "8.41.0",
    "eslint-config-prettier": "8.8.0",
    "jest": "29.5.0",
    "nx": "16.2.2",
    "prettier": "2.8.8",
    "shx": "^0.3.4",
    "standard-version": "^9.5.0",
    "ts-jest": "29.1.0",
    "typescript": "5.0.4"
  },
  "dependencies": {
    "rxjs": "^7.8.1"
  },
  "engines": {
    "node": "^18.0.0"
  }
}
