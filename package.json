    "@nrwl/eslint-plugin-nx": "14.8.6",
    "@nrwl/jest": "14.8.6",
    "@nrwl/js": "14.8.6",
    "@nrwl/linter": "14.8.6",
    "@nrwl/node": "14.8.6",
    "@nrwl/nx-plugin": "14.8.6",
    "@nrwl/workspace": "14.8.6",
    "@swc-node/register": "^1.4.2",
    "@swc/core": "^1.2.173",
    "@types/circular-dependency-plugin": "^5.0.5",
    "@types/copy-webpack-plugin": "^6.4.0",
    "@types/jest": "28.1.8",
    "@types/node": "18.7.1",
    "@types/rimraf": "^3.0.2",
    "@types/terser-webpack-plugin": "^4.2.0",
